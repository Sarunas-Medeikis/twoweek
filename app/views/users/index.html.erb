<h1>All users</h1>

<table border=1>
	<thead>
		<tr>
			<td>#</td>
			<td>Name</td>
			<td></td>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
		<tr>
			<td><%= user.id %></td>
			<td><%= user.name %></td>
			<td> <%= link_to 'Edit', edit_user_path(user) %> </td>
		<% end %>
		</tr>
	</tbody>
</table>

<p> <%= link_to 'Back To Articles', articles_path %> </p>
<p> <%= link_to 'New User', new_user_path %> </p>
